# Database Configuration
PAELLADOC_DB_PATH=~/.paelladoc/memory.db

# Environment
PAELLADOC_ENV=development

# Logging
PAELLADOC_LOG_LEVEL=INFO
PAELLADOC_LOG_FILE=paelladoc.log

# Optional: Vector Store Configuration (if using ChromaDB)
PAELLADOC_VECTOR_STORE_PATH=~/.paelladoc/vector_store

# Optional: Template Configuration
PAELLADOC_TEMPLATE_PATH=~/.paelladoc/templates